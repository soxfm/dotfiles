Host *
	Protocol 2
	UseRoaming no
	ForwardAgent no
	ConnectionAttempts 1
	ConnectTimeout 5
	UseKeychain yes
	AddKeysToAgent yes
	IdentitiesOnly yes
	HashKnownHosts yes
	VisualHostKey no
	UserKnownHostsFile ~/.ssh/known_hosts
#	PasswordAuthentication no
	ChallengeResponseAuthentication no
	StrictHostKeyChecking ask
	VerifyHostKeyDNS ask
	ForwardX11 no
	ForwardX11Trusted no
	TCPKeepAlive no
	Compression no
	Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com
	MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com
	KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256,curve25519-sha256,ecdh-sha2-nistp521
	HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com,rsa-sha2-512-cert-v01@openssh.com,ssh-ed25519,ecdsa-sha2-nistp521-cert-v01@openssh.com,rsa-sha2-512

